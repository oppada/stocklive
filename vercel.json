{
  "functions": {
    "server/index.js": {
      "runtime": "nodejs18.x"
    },
    "client/api/uapi/[...path].ts": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/uapi/:path*",
      "destination": "/client/api/uapi/[...path]"
    },
    {
      "source": "/api/:path*",
      "destination": "/server/index.js"
    }
  ]
}