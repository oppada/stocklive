{
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "client/api/uapi/[...path].ts",
      "use": "@vercel/node",
      "config": { "runtime": "edge" }
    }
  ],
  "routes": [
    {
      "src": "/api/uapi/(.*)",
      "dest": "/client/api/uapi/[...path]"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/$1"
    }
  ]
}