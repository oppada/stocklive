{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "functions": {
    "api/uapi/[...path].ts": {
      "runtime": "@vercel/edge"
    }
  },
  "routes": [
    {
      "src": "/api/uapi/(.*)",
      "dest": "/api/uapi/[...path]"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}